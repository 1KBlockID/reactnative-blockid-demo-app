{
  "name": "react-native-blockidplugin-example",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "build:android": "cd android && ./gradlew assembleDebug --no-daemon --console=plain -PreactNativeArchitectures=arm64-v8a",
    "build:ios": "cd ios && xcodebuild -workspace BlockidpluginExample.xcworkspace -scheme BlockidpluginExample -configuration Debug -sdk iphonesimulator CC=clang CPLUSPLUS=clang++ LD=clang LDPLUSPLUS=clang++ GCC_OPTIMIZATION_LEVEL=0 GCC_PRECOMPILE_PREFIX_HEADER=YES ASSETCATALOG_COMPILER_OPTIMIZATION=time DEBUG_INFORMATION_FORMAT=dwarf COMPILER_INDEX_STORE_ENABLE=NO",
    "postinstall": "npx patch-package"
  },
  "dependencies": {
    "@react-native/new-app-screen": "0.80.2",
    "@react-navigation/native": "^7.1.17",
    "@react-navigation/stack": "^7.4.5",
    "axios": "^1.11.0",
    "react": "19.1.0",
    "react-native": "0.80.2",
    "react-native-gesture-handler": "^2.27.2",
    "react-native-get-random-values": "^1.11.0",
    "react-native-permissions": "^5.4.2",
    "react-native-safe-area-context": "^5.5.2",
    "react-native-screens": "^4.13.1",
    "react-native-svg": "^15.12.1",
    "react-native-svg-circular-progress": "^1.0.4",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@react-native-community/cli": "19.1.1",
    "@react-native-community/cli-platform-android": "19.1.1",
    "@react-native-community/cli-platform-ios": "19.1.1",
    "@react-native/babel-preset": "0.80.2",
    "@react-native/metro-config": "0.80.2",
    "@react-native/typescript-config": "0.80.2",
    "@types/jest": "^29.5.13",
    "@types/react": "19.1.9",
    "@types/react-native-vector-icons": "^6.4.18",
    "@types/react-test-renderer": "19.1.0",
    "@types/uuid": "^10.0.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "jest": "^29.7.0",
    "react-test-renderer": "19.1.0"
  },
  "engines": {
    "node": ">=18"
  },
  "overrides": {
    "react-native": "0.80.2",
    "@react-native/virtualized-lists": "0.80.2",
    "@types/react": "19.1.9",
    "@types/react-dom": "19.1.9"
  },
  "resolutions": {
    "react-native": "0.80.2",
    "@react-native/virtualized-lists": "0.80.2",
    "@types/react": "19.1.9"
  }
}
